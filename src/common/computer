import * as $ from 'jquery-deferred';
import client from '../jiffClient';

interface computationOptions {
    party_id?: string;
    party_count?: number;
    secret_key?: string;
    public_key?: string;
    __internal_socket?: InternalSocket;
    [key: string]: any;
}

interface ComputationInstance {
    server: JIFFServer;
    [key: string]: any;
}

class InternalSocket {
    callbacks: Record<string, any>;
    jiff: JIFFServer;

    constructor(jiff: JIFFServer, computation_id: string) {
        this.callbacks = {};
        this.jiff = jiff;
    }
}

